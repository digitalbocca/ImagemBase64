{
  "name": "imagembase64",
  "version": "5.1.0",
  "description": "Entrega uma imagem em base64.",
  "main": "index.html",
  "config": {
    "commitMessage": "README e CHANGELOG seguem o padr√£o EDB2020."
  },
  "parcelCleanPaths": [
    "docs"
  ],
  "scripts": {
    "dev": "parcel ./src/index.pug -p 8080 --no-cache -d ./docs",
    "build": "parcel build ./src/index.pug --public-url ./ -d ./docs --no-source-maps --no-cache",
    "web": "http-server ./docs",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpb && npm run gpg",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpb": "git push bitbucket master --tags",
    "gpg": "git push gitlab master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/ImagemBase64.git"
  },
  "keywords": [
    "imagem",
    "image",
    "base64"
  ],
  "author": "Gabriel Bertola Bocca - gabriel at estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/ImagemBase64/issues"
  },
  "homepage": "https://github.com/digitalbocca/ImagemBase64#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "cross-var-no-babel": "^1.2.0",
    "cssnano-preset-advanced": "^4.0.7",
    "http-server": "^0.12.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-purgecss": "^3.0.0",
    "pug": "^3.0.0",
    "sass": "^1.26.7",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
